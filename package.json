{
	"name": "web-datamonkey",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"get-api": "wget https://raw.githubusercontent.com/veg/api-datamonkey/refs/heads/master/dist/openapi.yaml -O openapi.yaml",
		"gen-types": "npx @openapitools/openapi-generator-cli generate -i openapi.yaml -g typescript-jquery --skip-validate-spec",
		"update-generated-types": "run-s get-api gen-types"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"npm-run-all": "^4.1.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.5"
	}
}
